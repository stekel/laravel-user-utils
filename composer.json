{
    "name": "stekel/laravel-user-utils",
    "description": "Helpful commands for dealing with user accounts.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">7.1",
        "illuminate/console": "^5.6 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^5.6 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "stekel\\LaravelUserUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stekel\\LaravelUserUtils\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "stekel\\LaravelUserUtils\\Laravel\\Providers\\LaravelUserUtilsServiceProvider"
            ],
            "aliases": {}
        }
    }
}
